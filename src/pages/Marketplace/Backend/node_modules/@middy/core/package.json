  },
  "repository": {
    "type": "git",
    "url": "github:middyjs/middy",
    "directory": "packages/core"
  },
  "bugs": {
    "url": "https://github.com/middyjs/middy/issues"
  },
  "homepage": "https://middy.js.org",
  "funding": {
    "type": "github",
